#                       Password Generator
import random as rd
password_combo ="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789~!@#$%^&*()_-+=,./<>?';|:[{]}"
password = ""
for a in range(8):
    char = password_combo[rd.randint(0,len(password_combo)-1)]
    password += password.join(char)
print("Password:-",password)
